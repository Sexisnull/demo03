/* ::::: http://www.jsdaima.com/Upload/1476026502/Css/identify.css ::::: */

body { min-width: 1300px; }
/* ::::: http://www.jsdaima.com/Upload/1476026502/Css/layout.css ::::: */

html { margin: 0px; padding: 0px; border: 0px none; }
body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, code, del, dfn, em, q, dl, dt, dd, ol, ul, li, input, fieldset, form, legend, caption, tbody, tfoot, thead, article, aside, dialog, figure, footer, header, hgroup, nav, section { margin: 0px; padding: 0px; border: 0px none; font: inherit; vertical-align: baseline; }
body { font-size: 12px; color: rgb(0, 0, 0); background: rgb(255, 255, 255) none repeat scroll 0% 0%; font-family: "Microsoft YaHei","simsun","Helvetica Neue",Arial,Helvetica,sans-serif; }
img { border: 0px none; }
:-moz-placeholder { color: rgb(153, 153, 153); }
*::-moz-placeholder { color: rgb(153, 153, 153); }
* { box-sizing: border-box; }
*::before, *::after { box-sizing: border-box; }
button::-moz-focus-inner, input::-moz-focus-inner { padding: 0px; border: 0px none; }
b, strong { font-weight: bold; }
.clearfix::after { visibility: hidden; display: block; font-size: 0px; content: " "; clear: both; height: 0px; }
.fl { float: left !important; }
.fr { float: right !important; }
h2, .h2 { font-size: 24px; }
h3, .h3 { font-size: 18px; }
h6, .h6 { font-size: 12px; }
a { color: rgb(51, 51, 51); text-decoration: none; }
a:hover { color: rgb(9, 140, 186); }
p, .p { line-height: 25px; }
.text-normal { font-weight: normal; }
.text-center { text-align: center !important; }
.button { display: inline-block; padding: 6px 20px; border: 1px solid rgb(221, 221, 221); border-radius: 3px; color: rgb(58, 58, 58); font-size: 12px; line-height: 22px; text-align: center; background: transparent none repeat scroll 0% 0%; cursor: pointer; }
.button:hover { background: rgb(241, 241, 241) none repeat scroll 0% 0%; }
.padding-big { padding: 20px; }
.padding-big-top { padding-top: 20px; }
.padding-big-bottom { padding-bottom: 20px; }
.padding-big-left { padding-left: 20px; }
.padding-left { padding-left: 10px; }
.margin-big-top { margin-top: 20px; }
.margin-big-bottom { margin-bottom: 20px; }
.margin-left { margin-left: 10px; }
.margin-small-top { margin-top: 5px; }
ul { list-style: outside none none; }
.w50 { width: 50%; }
.w70 { width: 70%; }
.w80 { width: 80%; }
.w25 { width: 25%; }
.w100 { width: 100%; }
.text-default { font-size: 14px !important; }
.text-main { color: rgb(64, 178, 218); }
.text-white { color: rgb(255, 255, 255); }
/* ::::: http://www.jsdaima.com/Upload/1476026502/Css/account.css ::::: */

/* ::::: http://www.jsdaima.com/Upload/1476026502/Css/style.css ::::: */

body { font-family: "Lantinghei SC","microsoft yahei",Tahoma,Helvetica,SimSun,sans-serif; }
.icon-arrow-down, .icon-notice { display: block; width: 16px; height: 16px; background: transparent url('../images/icon_set.png') no-repeat scroll 0% 0%; }
.icon-arrow-down { background-position: 0px 0px; }
.icon-notice { background-position: -26px 0px; }
.view-topbar { position: fixed; width: 100%; min-width: 640px; height: 50px; background: rgb(42, 148, 222) none repeat scroll 0% 0%; z-index: 101; }
.view-body { position: absolute; width: 100%; min-width: 1200px; top: 50px; bottom: 0px; background-color: rgb(0, 0, 0); }
.view-body .view-sidebar { position: fixed; top: 50px; bottom: 0px; background-color: rgb(41, 48, 56); z-index: 102; }
.view-product { width: auto; position: absolute; top: 0px; left: 180px; bottom: 0px; right: 0px; padding-bottom: 50px; overflow: auto; background: rgb(255, 255, 255) none repeat scroll 0% 0%; }
.view-topbar .topbar-btn { color: rgb(255, 255, 255); font-size: 14px; line-height: 50px; }
.view-topbar .topbar-btn:hover, .view-topbar .topbar-btn.topbar-btn-dark { background: rgb(19, 128, 203) none repeat scroll 0% 0%; }
.dropdown { position: relative; }
.dropdown-menu { display: none; position: absolute; top: 100%; left: 0px; float: left; min-width: 160px; font-size: 12px; list-style: outside none none; background-color: rgb(255, 255, 255); border: 1px solid rgb(238, 238, 238); box-shadow: 0px 1px 3px rgb(229, 229, 229); z-index: 1000; }
.tobar-head { width: 180px; }
.tobar-head .topbar-logo { display: block; padding: 15px; width: 50px; height: 50px; background: rgb(9, 122, 200) none repeat scroll 0% 0%; }
.topbar-console .topbar-home-link { width: 130px; border-right: 1px solid rgb(0, 143, 191); }
.topbar-info { position: absolute; right: 0px; top: 0px; background: rgb(0, 143, 191) none repeat scroll 0% 0%; }
.topbar-info .topbar-notice { position: relative; margin-left: 1px; padding: 0px 20px; }
.topbar-info .topbar-notice .icon-notice { float: left; margin: 17px 5px 17px 0px; }
.topbar-info .topbar-notice .topbar-num { font-size: 12px; color: rgb(0, 172, 233); background: rgb(0, 135, 180) none repeat scroll 0% 0%; border-radius: 5px; padding: 2px 5px; display: inline-block; margin-top: 15px; line-height: 16px; vertical-align: top; text-align: center; }
.topbar-info .topbar-notice .topbar-num.have { color: rgb(255, 255, 255); background: rgb(255, 153, 0) none repeat scroll 0% 0%; }
.topbar-info .topbar-info-item { margin-left: 1px; }
.view-topbar .topbar-info .topbar-btn { padding: 0px 10px; height: 50px; display: block; z-index: 2; background: rgb(42, 148, 222) none repeat scroll 0% 0%; }
.topbar-info .topbar-btn:hover, .topbar-info .topbar-btn.topbar-btn-dark { background: rgb(19, 128, 203) none repeat scroll 0% 0%; }
.topbar-info .dropdown-menu { width: 100%; z-index: 1; }
.topbar-info .icon-arrow-down { float: right; margin: 17px 0px 17px 5px; }
.topbar-info .dropdown-menu { width: 100%; min-width: 0px; margin-top: -1px; }
.topbar-info .dropdown-menu li { border-bottom: 1px solid rgb(234, 237, 241); }
.topbar-info .dropdown-menu a { display: block; padding-left: 10px; color: rgb(51, 51, 51); line-height: 40px; }
.topbar-info .dropdown-menu a:hover { color: rgb(38, 38, 38); background-color: rgb(245, 245, 245); }
.sidebar-content { width: 180px; overflow: hidden; }
.sidebar-nav a { display: block; width: 100%; height: 40px; line-height: 40px; color: rgb(255, 255, 255); }
.sidebar-nav .sidebar-title a { background-color: rgb(34, 40, 46); }
.sidebar-nav .sidebar-title a:hover { background-color: rgb(65, 77, 92); }
.sidebar-nav .sidebar-title .icon { float: left; padding: 12px 17px; width: 50px; height: 40px; background: transparent none repeat scroll 0% 0%; }
.sidebar-nav .sidebar-title .icon b { transition: all 0.2s ease 0s; }
.sidebar-nav .sidebar-trans li a:hover { background-color: rgb(55, 66, 79); }
.sidebar-nav .sidebar-trans .sidebar-icon { float: left; padding: 12px 17px; width: 50px; height: 40px; opacity: 0.65; }
.sidebar-nav .sidebar-trans a:hover .sidebar-icon, .sidebar-nav .sidebar-trans li.active .sidebar-icon { opacity: 1; }
.sidebar-nav .sidebar-trans .sidebar-icon img { display: block; width: 16px; height: 16px; }
.margin-right-none { margin-right: 0px !important; }
.text-blue-deep { color: rgb(0, 152, 206); }
.text-red-deep { color: rgb(238, 98, 86); }
.icon { background: transparent url('../images/icon_control.png') no-repeat scroll 0% 0%; display: inline-block; }
/* ::::: http://www.jsdaima.com/Upload/1476026502/Css/control_index.css ::::: */

.view-product { min-width: 1075px; }
.background-color { background: rgb(242, 242, 242) none repeat scroll 0% 0%; height: 100%; }
.text-box-main { border: 1px solid rgb(230, 231, 233); background: rgb(255, 255, 255) none repeat scroll 0% 0%; }
.account-info .text-box-main { margin-right: 1%; }
.account-info dt { height: 88px; }
.account-info dt .title p { color: rgb(47, 47, 47); }
.min-width-300 { width: 49%; }
.min-width-360 { width: 49%; }
.button { border: 0px none; }
.button.icon { background: transparent url('../images/icon_control.png') no-repeat scroll 0% 0%; }
.button.bg-green-deep { background-color: rgb(30, 184, 107); color: rgb(255, 255, 255); }
.button.bg-green-deep:hover { background-color: rgb(52, 200, 126); }
.button.icon-button-green { background-position: 4px -71px; }
.account-info .button { padding: 2px 6px 2px 20px; border-radius: 4px; }
.account-info dt .icon-head img { width: 88px; height: 88px; }
.text-golden { color: rgb(255, 176, 0); }
.account-product .orange { color: rgb(234, 84, 74); }
.account-info dt .title .text-sliver { color: rgb(146, 146, 146); }
.account-info dd { height: 85px; }
.account-info dd i { display: inline-block; padding-top: 3px; width: 16px; height: 25px; }
.icon-mobile { background-position: 3px 5px; }
.icon-email { background-position: -24px 6px; }
.account-info .btn-red { padding: 0px 10px; margin-top: 2px; }
.btn-red { background: rgb(238, 88, 88) none repeat scroll 0% 0%; }
.btn-red:hover { color: rgb(255, 255, 255); background: rgb(235, 117, 110) none repeat scroll 0% 0%; }
.account-info .btn-orange { margin-top: 5px; padding: 6px 10px; }
.btn-orange { background: rgb(230, 126, 34) none repeat scroll 0% 0%; }
.btn-orange:hover { color: rgb(255, 255, 255); background: rgb(226, 136, 57) none repeat scroll 0% 0%; }
.text-hidden { width: 100%; height: 100%; overflow: hidden; }
.account-info .account-class a { margin-right: 25px; display: inline-block; line-height: 25px; }
.margin-right-none { margin-right: 0px !important; }
.account-product .mine-product { width: 99%; margin-right: 3%; min-height: 490px; }
.lists-border-list { border-bottom: 1px solid rgb(230, 231, 233); }
.border-list-text { line-height: 50px; padding: 0px 20px; }
.lists-list { width: 100%; height: 34px; line-height: 34px; overflow: hidden; }

