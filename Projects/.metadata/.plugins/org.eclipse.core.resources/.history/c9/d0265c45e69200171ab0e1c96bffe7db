package com.gsww.jup;

import com.hanweb.sso.ldap.util.MD5;


public class test {
	public static void main(String[] args) {
		   MD5 Md5 = new MD5();
		   String str = "gszw@888888";
		   System.out.println(Md5.getDecTxtChange(str));
		   System.out.println(Md5.getFromBASE64(str));
		   System.out.println(Md5.getMD5ofStr(str));
		   System.out.println(Md5.getTxtChang(str));
		   System.out.println(Md5.setFromBASE64(str));
		   System.out.println(Md5.encodeByJavaSecurity(str));
		   
	}
}
